<ul>
    <%userFiles.forEach(file => {%>
        <%if( file.fileid ){%>
                <!-- files -->
                <li><%= `${file.name}, ${file.size}, ${file.createdDate}` %></li>
            <%} else { %>
                <!-- folders -->
                <li><a href=<%=`/folders/${file.folderid}`%>><%= `${file.name}, ${file.size}, ${file.createdDate}` %></a></li>
        <%}%>                                    
    <%})%>                            
</ul>
                    