<ul>
    <%userFiles.forEach(file => {%>
        <%if( file.fileid ){%>
                <!-- files -->
                <li>
                    <a href=<%= `/files/${file.fileid}` %>>
                        <div class="name">
                            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><title>file-outline</title><path d="M14,2H6A2,2 0 0,0 4,4V20A2,2 0 0,0 6,22H18A2,2 0 0,0 20,20V8L14,2M18,20H6V4H13V9H18V20Z" /></svg>
                            <%=file.name%>
                        </div>
                        <div class="size">
                            <%=file.size ?? '..'%>
                        </div>
                        <div class="created">
                            <%=file.createdDate%>
                        </div>
                    </a>
                </li>
            <%} else { %>
                <!-- folders -->
                <li>
                    <a href=<%= `/folders/${file.folderid}` %>>
                        <div class="name">
                            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><title>folder-outline</title><path d="M20,18H4V8H20M20,6H12L10,4H4C2.89,4 2,4.89 2,6V18A2,2 0 0,0 4,20H20A2,2 0 0,0 22,18V8C22,6.89 21.1,6 20,6Z" /></svg>
                            <%=file.name%>
                        </div>
                        <div class="size">
                            <%=file.size?? '..'%>
                        </div>
                        <div class="created">
                            <%=file.createdDate%>
                        </div>
                    </a>
                </li>
        <%}%>                                    
    <%})%>                            
</ul>
                    