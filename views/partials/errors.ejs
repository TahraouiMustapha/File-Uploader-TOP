<%if(messages.error) {%>
    <ul class="errors">
        <%messages.error.forEach(msg => {%>
            <li><%= msg %></li>
        <%})%>
    </ul>
<%}%>

<%if(locals.errors) {%>
    <ul class="errors">
        <%errors.forEach(err=> {%>
            <li><%=err.msg%></li>
        <%})%>
    </ul>
<%}%> 